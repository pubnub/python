{
  ".pubnub.yml": [
    { "pattern": "^version: (.+)$", "cleared": true },
    { "pattern": "\\s+package-name: pubnub-(v?(\\d+\\.?){2,}([a-zA-Z0-9-]+(\\.?\\d+)?)?)$", "clearedPrefix": true },
    { "pattern": "/releases/download/(.*)/pubnub-.*.tar.gz$", "cleared": false },
    { "pattern": "/releases/download/.*/pubnub-(.*).tar.gz$", "clearedPrefix": true }
  ],
  "setup.py": [
    { "pattern": "^\\s{2,}version='(.+)',$", "clearedPrefix": true }
  ],
  "pubnub/pubnub_core.py": [
    { "pattern": "^\\s{2,}SDK_VERSION = \"(.+)\"$", "clearedPrefix": true }
  ]
}
